# Trello API Configuration
TRELLO_API_KEY=your_trello_api_key_here
TRELLO_API_TOKEN=your_trello_api_token_here
TRELLO_BOARD_ID=your_board_id_here
TRELLO_LIST_ID=your_list_id_here

# G2G URLs
G2G_ORDERS_URL=https://www.g2g.com/order/sellOrder?status=5
G2G_DELIVERING_URL=https://www.g2g.com/g2g-user/sale?status=delivering
G2G_BOOSTING_PREPARING_URL=https://www.g2g.com/order/sellOrder?status=6
G2G_BOOSTING_DELIVERING_URL=https://www.g2g.com/order/sellOrder?status=1
G2G_LOGIN_URL=https://www.g2g.com/login

# Server Configuration
PORT=8080

# Puppeteer Configuration
HEADLESS=false
USER_DATA_DIR=./user-data
# CHROME_EXECUTABLE_PATH=/path/to/chrome (optional, auto-detected if not set)

# Автоматическое сканирование заказов
# AUTO_SCAN_ENABLED=true - включить автоматическое сканирование (по умолчанию включено)
# SCAN_INTERVAL_MINUTES=5 - интервал сканирования в минутах (по умолчанию 5 минут)
AUTO_SCAN_ENABLED=true
SCAN_INTERVAL_MINUTES=5
